{
    "name": "portfolio",
    "version": "5.0.0",
    "description": "Built with Gatsby.js",
    "keywords": [
        "gatsby"
    ],
    "scripts": {
        "build": "gatsby build",
        "dev": "gatsby develop",
        "test": "jest",
        "coverage": "jest --coverage | codecov"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/aaronsky/portfolio"
    },
    "author": "aaronsky",
    "license": "MIT",
    "main": "n/a",
    "private": true,
    "bugs": {
        "url": "https://github.com/aaronsky/portfolio/issues"
    },
    "homepage": "https://skyaaron.com/",
    "dependencies": {
        "gatsby": "^2.0.0",
        "gatsby-plugin-favicon": "^3.1.0",
        "gatsby-plugin-nprogress": "^2.0.0",
        "gatsby-plugin-react-helmet": "^3.0.5",
        "gatsby-plugin-remove-trailing-slashes": "^2.0.0",
        "gatsby-plugin-typescript": "^2.0.0",
        "gatsby-source-filesystem": "^2.0.0",
        "gatsby-transformer-remark": "^2.0.0",
        "gatsby-transformer-yaml": "^2.0.0",
        "moment": "^2.23.0",
        "react": "^16.7.0",
        "react-dom": "^16.7.0",
        "react-helmet": "^5.2.0",
        "react-icons": "^3.2.0"
    },
    "devDependencies": {
        "@types/html-pdf": "^2.1.2",
        "@types/jest": "^23.0.0",
        "@types/node": "^10.12.0",
        "@types/react": "^16.7.0",
        "@types/react-dom": "^16.0.0",
        "@types/react-helmet": "^5.0.3",
        "codecov": "^3.0.0",
        "html-pdf": "^2.2.0",
        "husky": "^1.3.1",
        "jest": "^23.0.0",
        "prettier": "^1.15.3",
        "pretty-quick": "^1.8.0",
        "ts-jest": "^23.0.0",
        "typescript": "^3.5.2"
    },
    "jest": {
        "clearMocks": true,
        "coverageDirectory": "coverage",
        "globals": {
            "ts-jest": {
                "tsConfig": "tsconfig.json"
            }
        },
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js"
        ],
        "testEnvironment": "node",
        "testMatch": [
            "**/__tests__/*.+(ts|tsx|js)"
        ],
        "transform": {
            "^.+\\.(ts|tsx)$": "ts-jest"
        }
    },
    "husky": {
        "hooks": {
            "pre-commit": "pretty-quick --staged"
        }
    }
}
