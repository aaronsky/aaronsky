{
    "name": "portfolio",
    "version": "5.1.0",
    "description": "Built with Gatsby.js",
    "keywords": [
        "gatsby"
    ],
    "scripts": {
        "build": "GATSBY_TELEMETRY_DISABLED=1 gatsby build",
        "dev": "GATSBY_TELEMETRY_DISABLED=1 gatsby develop",
        "test": "jest",
        "coverage": "jest --coverage | codecov"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/aaronsky/portfolio"
    },
    "author": "aaronsky",
    "license": "MIT",
    "main": "n/a",
    "private": true,
    "bugs": {
        "url": "https://github.com/aaronsky/portfolio/issues"
    },
    "homepage": "https://skyaaron.com/",
    "dependencies": {
        "gatsby": "^2.22.4",
        "gatsby-plugin-favicon": "^3.1.6",
        "gatsby-plugin-humans-txt": "^1.1.4",
        "gatsby-plugin-nprogress": "^2.3.2",
        "gatsby-plugin-react-helmet": "^3.3.2",
        "gatsby-plugin-remove-trailing-slashes": "^2.3.3",
        "gatsby-plugin-typescript": "^2.4.3",
        "gatsby-source-filesystem": "^2.3.7",
        "gatsby-transformer-remark": "^2.8.12",
        "gatsby-transformer-yaml": "^2.4.2",
        "html-pdf": "^2.2.0",
        "moment": "^2.26.0",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "react-helmet": "^6.0.0",
        "react-icons": "^3.10.0",
        "typescript": "^3.9.3"
    },
    "devDependencies": {
        "@types/html-pdf": "^2.2.0",
        "@types/jest": "^25.2.3",
        "@types/node": "^14.0.4",
        "@types/react": "^16.9.35",
        "@types/react-dom": "^16.9.8",
        "@types/react-helmet": "^6.0.0",
        "codecov": "^3.7.0",
        "husky": "^4.2.5",
        "jest": "^26.0.1",
        "prettier": "^2.0.5",
        "pretty-quick": "^2.0.1",
        "ts-jest": "^26.0.0"
    },
    "jest": {
        "clearMocks": true,
        "coverageDirectory": "coverage",
        "globals": {
            "ts-jest": {
                "tsConfig": "tsconfig.json"
            }
        },
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js"
        ],
        "testEnvironment": "node",
        "testMatch": [
            "**/__tests__/*.+(ts|tsx|js)"
        ],
        "transform": {
            "^.+\\.(ts|tsx)$": "ts-jest"
        }
    },
    "husky": {
        "hooks": {
            "pre-commit": "pretty-quick --staged"
        }
    }
}
