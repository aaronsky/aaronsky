{
    "name": "portfolio",
    "version": "5.1.0",
    "description": "Built with Gatsby.js",
    "keywords": [
        "gatsby"
    ],
    "scripts": {
        "build": "GATSBY_TELEMETRY_DISABLED=1 gatsby build",
        "dev": "GATSBY_TELEMETRY_DISABLED=1 gatsby develop",
        "test": "jest",
        "coverage": "jest --coverage | codecov"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/aaronsky/portfolio"
    },
    "author": "aaronsky",
    "license": "MIT",
    "main": "n/a",
    "private": true,
    "bugs": {
        "url": "https://github.com/aaronsky/portfolio/issues"
    },
    "homepage": "https://skyaaron.com/",
    "dependencies": {
        "gatsby": "^2.18.16",
        "gatsby-plugin-favicon": "^3.1.6",
        "gatsby-plugin-humans-txt": "^1.1.4",
        "gatsby-plugin-nprogress": "^2.1.17",
        "gatsby-plugin-react-helmet": "^3.1.18",
        "gatsby-plugin-remove-trailing-slashes": "^2.1.17",
        "gatsby-plugin-typescript": "^2.1.23",
        "gatsby-source-filesystem": "^2.1.43",
        "gatsby-transformer-remark": "^2.6.45",
        "gatsby-transformer-yaml": "^2.2.20",
        "html-pdf": "^2.2.0",
        "moment": "^2.24.0",
        "react": "^16.12.0",
        "react-dom": "^16.12.0",
        "react-helmet": "^5.2.1",
        "react-icons": "^3.8.0",
        "typescript": "^3.7.4"
    },
    "devDependencies": {
        "@types/html-pdf": "^2.2.0",
        "@types/jest": "^24.0.24",
        "@types/node": "^12.12.21",
        "@types/react": "^16.9.17",
        "@types/react-dom": "^16.9.4",
        "@types/react-helmet": "^5.0.14",
        "codecov": "^3.6.5",
        "husky": "^3.1.0",
        "jest": "^24.9.0",
        "prettier": "^1.19.1",
        "pretty-quick": "^2.0.1",
        "ts-jest": "^24.2.0"
    },
    "jest": {
        "clearMocks": true,
        "coverageDirectory": "coverage",
        "globals": {
            "ts-jest": {
                "tsConfig": "tsconfig.json"
            }
        },
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js"
        ],
        "testEnvironment": "node",
        "testMatch": [
            "**/__tests__/*.+(ts|tsx|js)"
        ],
        "transform": {
            "^.+\\.(ts|tsx)$": "ts-jest"
        }
    },
    "husky": {
        "hooks": {
            "pre-commit": "pretty-quick --staged"
        }
    }
}
